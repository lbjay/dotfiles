
alias cp='cp -i'
alias mv='mv -i'
alias grep='grep --color'
alias cls='clear'
alias se='screen -e^Ee -S main -DR'
alias s='screen -DR'
alias vi='vim'
alias ports='lsof -i| grep LISTEN'
alias mkv='mkvirtualenv -a $(pwd) -p `pyenv which python` $(basename `pwd`)'
alias workong='workon'

