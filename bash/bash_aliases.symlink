# some more ls aliases
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -lCF'

# ipython notebook
alias ipynb="ipython notebook --notebook-dir=~/ipynb"

# Add an "alert" alias for long running commands.  Use like so:
#   sleep 10; alert
alias alert='notify-send --urgency=low -i "$([ $? = 0 ] && echo terminal || echo error)" "$(history|tail -n1|sed -e '\''s/^\s*[0-9]\+\s*//;s/[;&|]\s*alert$//'\'')"'

alias nbstrip_jq="jq --indent 1 \
    '(.cells[] | select(has(\"outputs\")) | .outputs) = []  \
    | (.cells[] | select(has(\"execution_count\")) | .execution_count) = null  \
    | .metadata = {\"language_info\": {\"name\": \"python\", \"pygments_lexer\": \"ipython3\"}} \
    | .cells[].metadata = {} \
    '"

alias oc-stacks='aws opsworks describe-stacks --query "Stacks[].[Name,StackId]" --output text'

# xrandr i3wm w/ dual monitor on/off
alias xlaptop='xrandr --verbose --output eDP-1 --primary --mode 1920x1080 --pos 320x1440 --rotate normal --output DP-1 --off --output HDMI-1 --off --output DP-2 --off --output HDMI-2 --off'
alias xdual='xrandr --verbose --output eDP-1 --primary --mode 1920x1080 --pos 320x1440 --rotate normal --output DP-1 --off --output HDMI-1 --off --output DP-2 --off --output HDMI-2 --mode 2560x1440 --pos 0x0 --rotate normal'
alias xoffice='bash ~/.screenlayout/office.sh'

# django
alias django-secret-key='python -c "import secrets; print(secrets.token_urlsafe())"'

# awscli v2
alias aws2='docker run --rm -it -v ~/.aws:/root/.aws -e AWS_PROFILE=test amazon/aws-cli'

# aws prod profile
alias aws-prod='aws --profile prod'
